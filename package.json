{
  "name": "@paperplane/cra-template-antd",
  "version": "3.0.0",
  "description": "A great Create-React-App template for antd.",
  "homepage": "",
  "author": "chiskat <1@paperplane.cc>",
  "type": "commonjs",
  "license": "MIT",
  "keywords": [
    "react",
    "create-react-app",
    "template",
    "typescript",
    "antd",
    "ant design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/paperplane-npm/cra-template-antd"
  },
  "bugs": {
    "url": "https://github.com/paperplane-npm/cra-template-antd/issues"
  },
  "files": [
    "template/**",
    "template.json"
  ],
  "scripts": {
    "dev": "npm run start",
    "start": "craco start",
    "build": "craco build",
    "eject": "craco eject",
    "format": "prettier --list-different \"src/**/*.{ts,tsx,css,scss,less}\" \"*.js\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"*.{js,mjs}\"",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "node ./cra-publish/makeCraTemplate.js",
    "release": "node ./cra-publish/publish.js"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@loadable/component": "^5.16.7",
