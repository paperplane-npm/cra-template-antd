    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@typescript-eslint/typescript-estree": "^8.53.0",
    "@craco/craco": "^7.1.0",
    "sass-loader": "^16.0.6",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@typescript-eslint/typescript-estree": "^8.53.0",
